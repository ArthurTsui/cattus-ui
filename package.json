{
  "name": "cattus-ui",
  "version": "0.0.3",
  "description": "Personal React UI library",
  "title": "Cattus UI",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "unpkg": "dist/cattus-ui.min.js",
  "repository": {
    "url": "https://github.com/ArthurTsui/cattus-ui",
    "type": "git"
  },
  "author": "LazyCat <1518623937@qq.com> (http://)",
  "license": "MIT",
  "keywords": [
    "raect",
    "ui",
    "cattus",
    "component",
    "components",
    "react",
    "react-component"
  ],
  "homepage": "",
  "bugs": "https://github.com/ArthurTsui/cattus-ui/issues",
  "files": [
    "dist",
    "es",
    "lib",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "dist/*",
    "es/**/styles/*",
    "lib/**/styles/*",
    "*.sass"
  ],
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "IE 11",
    "not IE 10"
  ],
  "scripts": {
    "build:link": "yarn build && yarn link",
    "build": "yarn clear && yarn build:umd && yarn build:es && yarn build:cjs && yarn build:gulp",
    "build:umd": "rimraf dist && yarn build:dev && yarn build:prod",
    "build:es": "rimraf es && tsc -p build/tsconfig/tsconfig.es.json --outDir es",
    "build:cjs": "rimraf lib && tsc -p build/tsconfig/tsconfig.cjs.json --outDir lib",
    "build:gulp": "cd build && gulp",
    "build:dev": "cross-env NODE_ENV=development webpack --config build/webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config build/webpack.config.js",
    "create:comp": "node scripts/createComp",
    "clear": "rimraf dist && rimraf lib && rimraf es",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "lint": "eslint src --fix --ext .ts,.tsx ",
    "publish:patch": "yarn build && yarn publish --new-version patch",
    "publish:minor": "yarn build && yarn publish --new-version minor",
    "publish:major": "yarn build && yarn publish --new-version major",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@types/classnames": "^2.3.1",
    "@types/enzyme": "^3.10.11",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-jest": "^27.4.5",
    "babel-loader": "^8.2.3",
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "cssnano": "^5.0.14",
    "docz": "^2.3.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-save": "^0.2.0",
    "fs": "^0.0.1-security",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "make-dir-cli": "^3.0.0",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "^7.0.1",
    "normalize-scss": "^7.0.1",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.45.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "through2": "^4.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "uppercamelcase": "^3.0.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
